
@{
    ViewBag.ActiveMenu = "AuditLogs"; //The menu item will be active for this page.
}
@section Styles{
    <link href="https://cdn.datatables.net/1.10.13/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css" rel="stylesheet" />
}
@section scripts
{
    <script src="//cdn.datatables.net/1.10.13/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.13/js/dataTables.bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.1.1/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.1.1/js/responsive.bootstrap.min.js"></script>

    <script>
        SystemJS.import('AuditLogs/Index.js').then(function (d) {
            new d.AuditLogsWindow().load();
        });
    </script>
}
<h1>@L("AuditLogs")</h1>
@if (ViewBag.TenantId != null)
{
    @Html.Hidden("TenantId", (int)ViewBag.TenantId)
}
<div class="row">
    <div class="col-md-12">
        <h2 class="lead" id="tenancyName"></h2>
        <table class="table table-condensed table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%" id="audit-logs-table"></table>
    </div>
</div>
